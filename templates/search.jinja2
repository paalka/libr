{% extends "base.jinja2" %}
{% block pageTitle %} search {% endblock %}
{% block header %}
<div id="search-bar-container">
  <div class="container section">
    <form role="form">
      <div class="form-group ten columns">
	<input tabindex="1" type="text" name="query" id="search-field" class="form-control" placeholder="" style="width: 100%">
      </div>
    </form>
  </div>
</div>
{% endblock %}
{% block body %}
<div class="container section">
  <div class="ten columns" id="search-result-container">
    {% include 'search_results.jinja2' %}
  </div>
</div>
{% endblock %}
