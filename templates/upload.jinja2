{% extends "base.jinja2" %}
{% block body %}
<div class="container section">
  <div class="row">
    <h4>Upload new file</h1>
    <form method=post enctype=multipart/form-data>
    <input type=file name=file>
    <div class="row">
	<input type="text" name="title" placeholder="File title">
    </div>
    <div class="row">
	<input type=submit value=Upload>
    </div>
    </form>
  </div>
</div>
{% endblock %}
